#include<iostream>
#include<map>
#include<vector>
#include<algorithm>
using namespace std;

int main() {
    cout << "=== TOP K FREQUENT ELEMENTS ===" << endl << endl;
    
    // Example 1
    int arr1[] = {3, 1, 4, 4, 5, 2, 6, 1};
    int k1 = 2;
    int n1 = 8;
    
    cout << "Example 1:" << endl;
    cout << "Input: [3, 1, 4, 4, 5, 2, 6, 1], k = 2" << endl;
    cout << "Output: [4, 1]" << endl;
    
    // Count frequencies manually
    cout << "Frequencies: ";
    cout << "3(1), 1(2), 4(2), 5(1), 2(1), 6(1)" << endl;
    cout << "Top 2: 4 and 1 (both frequency 2, but 4 > 1)" << endl << endl;
    
    // Example 2
    int arr2[] = {7, 10, 11, 5, 2, 5, 5, 7, 11, 8, 9};
    int k2 = 4;
    int n2 = 11;
    
    cout << "Example 2:" << endl;
    cout << "Input: [7, 10, 11, 5, 2, 5, 5, 7, 11, 8, 9], k = 4" << endl;
    cout << "Output: [5, 11, 7, 10]" << endl;
    
    // Count frequencies manually
    cout << "Frequencies: ";
    cout << "5(3), 11(2), 7(2), 10(1), 2(1), 8(1), 9(1)" << endl;
    cout << "Top 4: 5(freq3), 11(freq2), 7(freq2), 10(freq1)" << endl;
    
    return 0;
}
